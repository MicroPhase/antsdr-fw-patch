diff --git a/arch/arm/boot/dts/zynq-ant.dts b/arch/arm/boot/dts/zynq-ant.dts
new file mode 100644
index 000000000000..d9ee46e79b33
--- /dev/null
+++ b/arch/arm/boot/dts/zynq-ant.dts
@@ -0,0 +1,62 @@
+// SPDX-License-Identifier: GPL-2.0
+/*
+ *
