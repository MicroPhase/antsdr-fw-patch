From: jcc <995586238@qq.com>
Date: Fri, 9 Dec 2022 13:39:48 +0800
Subject: [PATCH] add uboot support

---
 arch/arm/dts/Makefile         |   1 +
 arch/arm/dts/zynq-ant-sdr.dts | 113 ++++++++++++++++++++++++++++++++++
 configs/zynq_ant_defconfig    |  62 +++++++++++++++++++
 include/configs/zynq-common.h |   8 +++
 4 files changed, 184 insertions(+)
 create mode 100644 arch/arm/dts/zynq-ant-sdr.dts
 create mode 100644 configs/zynq_ant_defconfig

diff --git a/arch/arm/dts/Makefile b/arch/arm/dts/Makefile
index 7b19efc9b8..c5734e94b7 100644
--- a/arch/arm/dts/Makefile
+++ b/arch/arm/dts/Makefile
@@ -96,6 +96,7 @@ dtb-$(CONFIG_ARCH_ZYNQ) += zynq-zc702.dtb \
 	zynq-picozed.dtb \
 	zynq-picozed-sdr2.dtb \
 	zynq-pluto-sdr.dtb \
+	zynq-ant-sdr.dtb \
 	zynq-m2k.dtb \
 	zynq-sidekiqz2.dtb \
 	zynq-zc770-xm010.dtb \
diff --git a/arch/arm/dts/zynq-ant-sdr.dts b/arch/arm/dts/zynq-ant-sdr.dts
new file mode 100644
index 0000000000..5490d38aa7
--- /dev/null
+++ b/arch/arm/dts/zynq-ant-sdr.dts
@@ -0,0 +1,113 @@
+/*
+ * MicroPhase Inc. ANTSDR board DTS
+ *
+ * Copyright (C) 2015-2018 MicroPhase Inc.
+ *
+ * SPDX-License-Identifier:	GPL-2.0+
+ */
+/dts-v1/;
+#include "zynq-7000.dtsi"
+
+/ {
+	model = "MicroPhase, ANTSDR-E310";
+	compatible = "xlnx,zynq-7000";
+
+	aliases {
+		ethernet0 = &gem0;
+		serial0 = &uart1;
+		spi0 = &qspi;
+	};
+
+	memory {
+		device_type = "memory";
+		reg = <0x0 0x40000000>;
+	};
+
+	chosen {
+		bootargs = "earlyprintk";
+		linux,stdout-path = &uart1;
+		stdout-path = &uart1;
+	};
+
+	usb_phy0: phy0 {
+		compatible = "usb-nop-xceiv";
+		#phy-cells = <0>;
+	};
+	gpio_keys {
+		compatible = "gpio-keys";
+		#address-cells = <1>;
+		#size-cells = <0>;
+		button {
+			label = "Button";
+			gpios = <&gpio0 14 0>;
+			linux,code = <103>; /* up */
+			wakeup-source;
+			autorepeat;
+		};
+
+	};
+	
+	
+};
+
+
+
+	
+
+
+&qspi {
+	status = "okay";
+	is-dual = <0>;
+	num-cs = <1>;
+	flash@0 {
+		compatible = "n25q512a","micron,m25p80";
+		reg = <0x0>;
+		spi-tx-bus-width = <1>;
+		spi-rx-bus-width = <4>;
+		spi-max-frequency = <50000000>;
+		#address-cells = <1>;
+		#size-cells = <1>;
+		partition@qspi-fsbl-uboot {
+			label = "qspi-fsbl-uboot";
+			reg = <0x0 0x100000>; /* 1M */
+		};
+		partition@qspi-uboot-env {
+			label = "qspi-uboot-env";
+			reg = <0x100000 0x20000>; /* 128k */
+		};
+		partition@qspi-nvmfs {
+			label = "qspi-nvmfs";
+			reg = <0x120000 0xE0000>; /* 1M */
+		};
+		partition@qspi-linux {
+			label = "qspi-linux";
+			reg = <0x200000 0x1E00000>; /* 30M */
+		};
+	};
+};
+
+&uart1 {
+	status = "okay";
+};
+
+&usb0 {
+	status = "okay";
+	dr_mode = "host";
+	usb-phy = <&usb_phy0>;
+};
+&sdhci0 {
+	u-boot,dm-pre-reloc;
+	status = "okay";
+	
+};
+
+&gem0 {
+	status = "okay";
+	phy-mode = "rgmii-id";
+	phy-handle = <&ethernet_phy0>;
+
+	ethernet_phy0: ethernet-phy@0 {
+		reg = <0>;
+		reset-gpios = <&gpio0 46 0>;
+	};
+};
diff --git a/configs/zynq_ant_defconfig b/configs/zynq_ant_defconfig
new file mode 100644
index 0000000000..6e43bd6a1c
--- /dev/null
+++ b/configs/zynq_ant_defconfig
@@ -0,0 +1,62 @@
+CONFIG_ARM=y
+CONFIG_SYS_CONFIG_NAME="zynq_zc70x"
